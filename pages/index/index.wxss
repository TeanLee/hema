  background-color: #ff9292;
}
.c2ec4c6 {
  background-color: #2ec4c6;
}
.c8599b9 {
  background-color: #8599b9;
}
.f3d24c {
  background-color: #f3d24c;
}
.page__sort__image image {
  width: 96rpx;
  height: 96rpx;
  transform: rotate(-45deg);
  border-bottom-left-radius: 100rpx;
  border-bottom-right-radius: 100rpx;
}
.page__bd ul.page__bd__sorts li.page__bd__sort p {
  font-size: 24rpx;
  color: #535353;
}
.page__bd__scrollx-section .scrollx-section__title {
  width: 682rpx;
  height: 70rpx;
  margin-top: 100rpx;
  margin-bottom: 50rpx;
  /* outline: 1px solid red; */
  margin-left: 38rpx;
  line-height: 70rpx;
}
.page__bd__scrollx-section .scrollx-section__title__left image {
  width: 50rpx;
  height: 60rpx;
}
.page__bd__scrollx-section .scrollx-section__title__left p {
  font-size: 32rpx;
  font-weight: bold;
  margin-left: 20rpx;
  line-height: 70rpx;
}
.page__bd__scrollx-section .scrollx-section__title__right p {
  font-size: 24rpx;
  font-weight: bold;
}
.page__bd__scrollx-section .scrollx-section__title__right a {
  width: 34rpx;
  height: 34rpx;
  background-color: #222222;
  border-radius: 18rpx;
  display: inline-block;
  margin-top: 16rpx;
  margin-left: 8rpx;
}
.page__bd__scrollx-section .scrollx-section__title__right a:after {
  content: '';
  float: right;
  margin-right: 12rpx;
  margin-top: 10rpx;
  display: inline-block;
  width: 12rpx;
  height: 12rpx;
  border-top: 1px solid #ffffff;
  border-right: 1px solid #ffffff;
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
}
.scrollx-section__content {
  width: 100%;
  white-space: nowrap;
}
.scrollx-section__content__item {
  width: 626rpx;
  height: 240rpx;
  /* outline: 1px solid red; */
  display: inline-block;
  margin-left: 20rpx;
  overflow: hidden;
}
.scrollx-section__content__item.last {
  margin-right: 20rpx;
}
.scrollx-section__content .scrollx-section__content__item .scrollx-section__item__wrapper {
  width: 572rpx;
  height: 222rpx;
  /* outline: 1px solid purple; */
  margin-left: 30rpx;
  margin-top: 40rpx;
  overflow: hidden;
}
.view__wrapper__image {
  width: 210rpx;
  height: 180rpx;
  /* outline: 1px solid blue; */
  margin-top: 20rpx;
  /* background-image: url("https://img.alicdn.com/imgextra/i1/TB1l8pVRXXXXXXcXFXXXXXXXXXX_!!0-item_pic.jpg_430x430q90.jpg"); */
}
.view__wrapper__image image {
  width:210rpx;
  height: 180rpx;
}
.view__wrapper__intro {
  width: 336rpx;
  height: 222rpx;
  /* outline: 1px solid green; */
  float: right;
  margin-top: -200rpx;
  overflow: hidden;
}
.wrapper__intro__title {
  padding-top: 30rpx;
  width: 318rpx;
}
.wrapper__intro__title text {
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.wrapper__intro__content{
  width: 166rpx;
  height: 64rpx;
}
.scrollx-section__content__item text {
  display: block;
}
.wrapper__intro__title text:nth-child(2n) {
  font-size: 24rpx;
  color: #a9a9a9;
  font-weight: blod;
}
.wrapper__intro__title text:nth-child(2n+1) {
  font-size: 28rpx;
  font-weight: blod;
  color: #393939;
}
.wrapper__intro__content text:nth-child(2n+1) {
  font-size: 34rpx;
  font-weight: 800;
  color: #434343;
}
.wrapper__intro__content text:nth-child(2n) {
  font-size: 22rpx;
  color: #a5a5a5;
}
.wrapper__intro__price {
  width: 100%;
  display: block;
  overflow: hidden;
}
.wrapper__intro__price a {
  color: #ff7c2a;
  font-size: 26rpx;
  font-weight: 600;
}
.wrapper__intro__price a:nth-child(2) {
  color: #a6a6a6;
  font-size: 24rpx;
}
.wrapper__intro__price a:nth-child(3) {
  color: #25a4ff;
  font-size: 40rpx;
  float: right;
  font-weight: 800;
  /* margin-top: 6rpx; */
  margin-right: 30rpx;
}

.van-card {
  padding: 0 !important;
}

.van-cell {
  padding: 0 !important;
}
.container {
  width: 100%;
  height: 100%;
}
.page__hd {
  width: 100%;
  height: 88rpx;
  background-color: #0eb6ff;
}
.left {
  float: left;
  display: inline-block;
}
.page__hd image {
  width: 44rpx;
  height: 44rpx;
  margin: 6rpx 28rpx;
}
.page__hd-input {
  width: 544rpx;
  height: 66rpx;
  background-color: #ffffff;
  border-radius: 33rpx;
  font-size: 32rpx;
  color: #cbcbcb;
  text-align: center;
  line-height: 66rpx;
}
.page__hd-input image {
  width: 32rpx;
  height: 32rpx;
  margin: 0;
}
.page__bd .page__bd__scroll image {
  width: 100%;
  height: 300rpx;
}
.page__bd .page__scroll__btns {
  width: 170rpx;
  height: 16rpx;
  position: absolute;
  top: 354rpx;
  left: 280rpx;
}
.page__bd .page__scroll__btns .page__scroll__btn {
  float: left;
  display: inline-block;
  width: 14rpx;
  height: 14rpx;
  border: 1px solid #8e8d7f;
  border-radius: 7rpx;
  margin: 0 8rpx;
}
.active {
  background-color: #23a3ff;
  border-color: #23a3ff;
}
.page__bd .page__bd__notice {
  width: 100%;
  height: 80rpx;
  background-color: #fff4d1;
  color: #878887;
  font-size: 30rpx;
  overflow: hidden;
}
.page__bd .page__bd__notice image {
  width: 40rpx;
  height: 40rpx;
  margin: 20rpx;
  display: inline-block;
  float: left;
}
.page__bd .page__bd__notice .page__notice__content {
  display: block;
  width: 600rpx;
  height: 40rpx;
  float: left;
  margin-top: 14rpx;
  white-space:nowrap; 
  overflow:hidden; 
  text-overflow:ellipsis;
}
.page__bd .page__bd__notice .page__notice__right {
  display: block;
  width: 22rpx;
  height: 22rpx;
  font-size: 20rpx;
  float: right;
  margin-top: 24rpx;
  margin-right: 44rpx;
}
.page__bd ul.page__bd__sorts {
  width: 700rpx;
  height: 320rpx;
  /* border: 1px solid red; */
  display: block;
  padding: 24rpx;
  margin-top: -70rpx;
}
.page__bd ul.page__bd__sorts li.page__bd__sort {
  width: 100rpx;
  height: 144rpx;
  /* outline: 1px solid black; */
  margin-top: 10rpx;
  margin-right: 50rpx;
}
.page__bd ul.page__bd__sorts li.page__bd__sort:nth-child(5n) {
  margin-right: 0;
  margin-bottom: 32rpx;
}
.page__bd ul.page__bd__sorts li.page__bd__sort .page__sort__image {
  width: 90rpx;
  height: 90rpx;
  border-radius: 100rpx;
  /* background-color: #be8159; */
}
.be8159 {
  background-color: #be8159;
}
.a1b1ca {
  background-color: #a1b1ca;
}
.ffc793 {
  background-color: #ffc793;
}
